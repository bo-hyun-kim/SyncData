<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
     PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
     "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nexmotion.position.PositionMapper">
    <resultMap id="position" type="com.nexmotion.position.Position">
        <id column="cnmcode" property="cnmcode" />
    </resultMap>

  <insert id="insertPosition" parameterType="java.util.List">
     /* ViCnmCdMapper.insertViCmnCd */
     INSERT IGNORE
       INTO VI_CNM_CD
           (CMN_CD
           ,CMN_CD_NM
           ,GRP_CD
           ,chgdate
           ,useyn)
     VALUES
     <foreach collection="list" item="item" index="index" separator=",">
          (#{item.grpcode}
          ,#{item.cmdcode}
          ,#{item.cmdcodename}
          ,#{item.chgdate}
          ,#{item.useyn})
     </foreach>
  </insert>

    <select id="selectPosition" resultMap="position">
        /* positionMapper.selectPosition */
        SELECT cnmcode
        FROM VI_CNM_CD
    </select>
</mapper>