<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
     PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
     "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="com.nexmotion.deletedata.DeleteDataMapper">

    <delete id="deletedata" parameterType="com.nexmotion.deletedata.DeleteDataMapper">
        /*DeleteDataMapper.deletedata*/
        DELETE
          FROM account
         WHERE userstat = '03'
           AND chgdate <![CDATA[<=]]> #{thresholdDate}
    </delete> 
</mapper>